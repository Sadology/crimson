    "canvacord": "^5.2.3",
    "discord-api-types": "^0.26.1",
    "discord-ytdl-core": "^5.0.4",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "ffmpeg-static": "^4.4.1",
    "fs": "^0.0.1-security",
    "libsodium-wrappers": "^0.7.9",
